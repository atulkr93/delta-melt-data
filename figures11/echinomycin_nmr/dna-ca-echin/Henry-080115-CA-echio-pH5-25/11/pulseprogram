# 1 "mc_line 53 file /opt/topspin3.1/exp/stan/nmr/lists/pp/noesyfpgpphwg exp. def. part of mc cmd. before ze"
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter ST1CNT
"ST1CNT = td1/(2)"
"MCWRK = 0.142857*d1"
"MCREST = d1 - d1"

    dccorr
