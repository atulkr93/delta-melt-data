# 1 "mc_line 116 file /opt/topspin3.1/exp/stan/nmr/lists/pp/noesyfpgpphwg exp. mc cmd. in line"
  MCWRK  wr #0 if #0 zd 

  "ph1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval1 = (ph1loop % 4) * 90"
      MCWRK ip1 + phval1
      "phval7 = (ph1loop % 4) * 90"
      MCWRK ip7 + phval7
  lo to LBLF1 times 2
  MCWRK 

  "t1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval1 = (ph1loop % 4) * 90"
      MCWRK ip1 + phval1
      "phval7 = (ph1loop % 4) * 90"
      MCWRK ip7 + phval7
  lo to LBLST1 times ST1CNT
  MCWRK 
  "t1loop=0"
  "ph1loop=0"
  MCWRK
