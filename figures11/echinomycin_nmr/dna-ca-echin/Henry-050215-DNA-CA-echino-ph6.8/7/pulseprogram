# 80 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/Delay.incl"
;$Id: Delay.incl,v 1.13.8.2 2013/08/30 09:44:49 ber Exp $
# 23 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na" 2


# 26 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
"d11=30m"
"d12=20u"
"d21=1s/(cnst4*2)"


"in0=inf1"
# 33 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
"d0=in0/2-p21*4/3.1415"


"DELTA1=d21-p16-d16-p39*cnst39"
"DELTA2=p39*cnst39-de-4u"
# 40 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
"spoff23=bf1*(cnst19/1000000)-o1"
"spoff24=bf1*(cnst19/1000000)-o1"


# 1 "mc_line 44 file C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na exp. def. part of mc cmd. before ze"
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter ST1CNT
"ST1CNT = td1 / 2"
"MCREST = d1 - d1"
"MCWRK = 0.200000*d1"

    dccorr
# 44 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
1 ze 
# 1 "mc_line 44 file C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na exp. def. of mc cmd. after ze"
      "d0=d0orig + t1loop * in0 "
      "phval3 = (ph1loop % 4) * 90"
      MCWRK ip3 + phval3
# 45 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
# 1 "mc_line 45 file C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na exp. start label for mc cmd."
2 MCWRK  * 2 do:f3
LBLF1, MCWRK  * 2
LBLST1, MCWRK 
  MCREST
# 46 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
3 d12 pl2:f2 pl3:f3 
  50u setnmr3|0 setnmr0|34|32|33 ctrlgrad 0
# 49 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
  p16:gp2
  d16

  (p39:sp23 ph1):f1
  p16:gp1
  d16

# 57 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
  (center (p40:sp24 ph2):f1 (p8 ph1):f2 (DELTA1 p21 ph3 d0 p21 ph4 DELTA1):f3 )


# 63 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
  DELTA2
  p16:gp1
  d16 pl26:f3
  4u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7
  go=2 ph31 cpd3:f3 
# 1 "mc_line 68 file C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na exp. mc cmd. in line"
  MCWRK  do:f3 wr #0 if #0 zd 

  "ph1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval3 = (ph1loop % 4) * 90"
      MCWRK ip3 + phval3
  lo to LBLF1 times 2
  MCWRK 

  "t1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval3 = (ph1loop % 4) * 90"
      MCWRK ip3 + phval3
  lo to LBLST1 times ST1CNT
  MCWRK 
  "t1loop=0"
  "ph1loop=0"
  MCWRK
# 70 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
exit 
  

ph1=0 
ph2=0 
ph3=0 2
ph4=0 0 2 2 
ph31=0 2 2 0

# 80 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
;pl3 : f3 channel - power level for pulse (default)
;pl26: f3 channel - power level for CPD/BB decoupling (low power)
;sp13: f2 channel - shaped pulse 180 degree (adiabatic)
;sp23: f1 channel - shaped pulse 120 degree 
;                   (Pc9_4_120.1000)
;sp24: f1 channel - shaped pulse 180 degree (Rsnob.1000)
;p8 : f2 channel - 180 degree hard pulse for inversion
;p16: homospoil/gradient pulse                       [1 msec]
;p21: f3 channel -  90 degree high power pulse
;p39: f1 channel - 120 degree shaped pulse for excitation
;                      Pc9_4_120.1000 (120o)    (3.0ms at 600.13 MHz)
;p40: f1 channel - 180 degree shaped pulse for refocussing
;                      Rsnob.1000               (1.0ms at 600.13 MHz)
;d0 : incremented delay (2D) = in0/2-p21*4/3.1415
;d1 : relaxation delay
;d11: delay for disk I/O                             [30 msec]
;d12: delay for power switching                      [20 usec]
;d16: delay for homospoil/gradient recovery
;d21 : 1/(2J)NH
;cnst4: = J(NH)
;cnst19: H(N) chemical shift (offset, in ppm)
;cnst39: compensation of chemical shift evolution during p39
;           Pc9_4_120.1000: 0.529
;inf1: 1/SW(N) = 2 * DW(N)
;in0: 1/ SW(N) = 2 * DW(N)
;nd0: 1
;ns: 2 * n
;ds: 16
;aq: <= 50 msec
;td1: number of experiments
;FnMODE: States-TPPI, TPPI, States or QSEC
;cpd3: decoupling according to sequence defined by cpdprg3: garp4.p62
;pcpd3: f3 channel - 90 degree pulse for decoupling sequence
;          use pulse of >= 350 usec


;use gradient ratio:	gp 1 : gp 2
;			  11 :    7
# 120 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
;for z-only gradients:
;gpz1: 11%
;gpz2:  7%

;use gradient files:   
;gpnam1: SMSQ10.100
;gpnam2: SMSQ10.100

# 129 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/sfhmqcf3gpph_na"
                                          ;preprocessor-flags-start
;1: for C-13 and N-15 labeled samples start experiment with
;             option -DLABEL_CN (eda: ZGOPTNS)
                                          ;preprocessor-flags-end


