2 MCWRK  * 2 do:f2  do:f3
LBLF1, MCWRK  * 2
LBLST1, MCWRK 
  MCREST
# 46 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/noesyesgpph_dec_ijk"
3 d12 pl1:f1 pl12:f2 pl16:f3
  p1 ph1
  d0 cpd2:f2 cpd3:f3
  p1 ph2             
  d8 do:f2 do:f3
  p1 ph3
# 53 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/noesyesgpph_dec_ijk"
