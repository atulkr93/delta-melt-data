# 72 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/noesyesgpph_dec_ijk"
  p2 ph7

  4u
  p16:gp2
  d16
  4u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7

  go=2 ph31 cpd2:f2 cpd3:f3
