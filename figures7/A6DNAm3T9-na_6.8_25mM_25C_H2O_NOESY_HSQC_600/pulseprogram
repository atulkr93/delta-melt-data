# 1 "mc_line 44 file C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/noesyesgpph_dec_ijk exp. def. part of mc cmd. before ze"
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter ST1CNT
"ST1CNT = td1 / 2"
"MCREST = d1 - d1"
"MCWRK = 0.200000*d1"

    dccorr
# 44 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/noesyesgpph_dec_ijk"
1 ze
# 1 "mc_line 44 file C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/noesyesgpph_dec_ijk exp. def. of mc cmd. after ze"
      "d0=d0orig + t1loop * in0 "
      "phval1 = (ph1loop % 4) * 90"
      MCWRK ip1 + phval1
# 45 "C:/Bruker/TopSpin3.2/exp/stan/nmr/lists/pp/user/noesyesgpph_dec_ijk"
